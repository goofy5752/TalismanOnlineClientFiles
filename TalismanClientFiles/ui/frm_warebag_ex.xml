        <OnEvent>if event == "CEV_BANK_RECV_DATA" then
					layWorld_frmWarebagEx_Refresh()
				elseif event == "CEV_BANK_RECVITEM_UPDATE_ULTRABAG" then
					layWorld_frmWarebagEx_Refresh()
